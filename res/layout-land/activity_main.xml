<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center" >

    <TableRow
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/presentation_textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="50dp"
            android:drawableLeft="@drawable/voting"
            android:drawablePadding="5dp"
            android:drawableStart="@drawable/voting"
            android:gravity="center_vertical"
            android:text="@string/title_activity_presentation"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/evaluation_textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/evaluate"
            android:drawablePadding="5dp"
            android:drawableStart="@drawable/evaluate"
            android:gravity="center_vertical"
            android:text="@string/title_activity_evaluation"
            android:textSize="22sp" />
    </TableRow>

    <TableRow
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >
		
        <TextView
            android:id="@+id/question_gen_textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:layout_marginRight="50dp"
            android:drawableLeft="@drawable/question_gen"
            android:drawablePadding="5dp"
            android:drawableStart="@drawable/question_gen"
            android:gravity="center_vertical"
            android:text="@string/title_activity_planning"
            android:textSize="22sp" />
        
        <TextView
            android:id="@+id/setting_textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:drawableLeft="@drawable/settings"
            android:drawablePadding="5dp"
            android:drawableStart="@drawable/settings"
            android:gravity="center_vertical"
            android:text="@string/title_activity_settings"
            android:textSize="22sp" />
        
    </TableRow>

</TableLayout>